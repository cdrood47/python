    "    def __init__(self, name, age = 5,time = 10):\n",
    "        print(self, 'is Good')\n",
    "        self.name = name\n",
    "        self.age = age\n",
    "        self.time = time\n",
    "        \n",
    "    def sleep(self):\n",
    "        print('self:',self)\n",
    "        print (self.name, '은 잠을 잔다')\n",
    "    def game(self):\n",
    "        print('self:',self)\n",
    "        print(self.name, '은',self.time,'분 동인 게임을 했다')\n",
    "        "
   ]
